{
  "cSpell.words": [
    "asan",
    "Celero",
    "cloc",
    "CMake",
    "cmakelang",
    "ctest",
    "DCMAKE",
    "DNUM",
    "doxygen",
    "Eigen",
    "fcoverage",
    "fprofile",
    "fsanitize",
    "gemnasium",
    "gitmessage",
    "htmlreport",
    "interp",
    "Kabashima",
    "Kenta",
    "libcxx",
    "libstdc",
    "mkdir",
    "MusicScience37",
    "pipenv",
    "Pipfile",
    "profraw",
    "Pybind11",
    "Tikhonov",
    "tsan",
    "ubsan",
    "venv"
  ],
  "clangd.arguments": [
    "--compile-commands-dir=${workspaceFolder}/build/Debug",
    "--clang-tidy",
    "-header-insertion=iwyu"
  ],
  "cmake.buildDirectory": "${workspaceFolder}/build/${buildType}",
  "cmake.preferredGenerators": ["Ninja"],
  "restructuredtext.linter.name": "doc8",
  "restructuredtext.languageServer.disabled": true,
  "restructuredtext.confPath": "${workspaceFolder}/doc/sphinx/src",
  "doxdocgen.generic.returnTemplate": "\\return ",
  "doxdocgen.generic.paramTemplate": "\\param[in] {param} ",
  "doxdocgen.c.firstLine": "/*!",
  "doxdocgen.c.factoryMethodText": "",
  "doxdocgen.c.getterText": "Get the {name}.",
  "doxdocgen.c.setterText": "Set the {name}.",
  "doxdocgen.c.triggerSequence": "/*!",
  "doxdocgen.cpp.ctorText": "Construct.",
  "doxdocgen.cpp.dtorText": "Destruct."
}
